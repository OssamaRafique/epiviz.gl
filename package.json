{
  "name": "epiviz.gl",
  "version": "1.0.0",
  "repository": "https://github.com/epiviz/epiviz.gl",
  "homepage": "https://github.com/epiviz/epiviz.gl",
  "author": "Sam Rosen",
  "license": "MIT",
  "type": "module",
  "source": "src/index.js",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "app": "dist-app/index.html",
  "targets": {
    "app": {
      "context": "browser",
      "publicUrl": "./",
      "optimize": false
    }
  },
  "scripts": {
    "watch": "parcel watch",
    "build": "yarn clean && parcel build",
    "clean": "rm -rf dist dist-app",
    "serve-app": "parcel serve app/index.html --open --dist-dir dist-app",
    "build-app": "parcel build app/index.html --dist-dir dist-app",
    "predeploy": "yarn clean && yarn build && yarn build-app",
    "deploy": "gh-pages -d dist-app",
    "copy": "source .env && rsync -qazP $DEPLOY_FILES $DEPLOY_TARGET",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "@turf/boolean-point-in-polygon": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "@turf/simplify": "^6.5.0",
    "d3-axis": "^3.0.0",
    "d3-color": "^3.0.1",
    "d3-format": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.0.0",
    "d3-selection": "^3.0.0",
    "flatbush": "^3.3.0",
    "fpsmeter": "^0.3.1",
    "jsonschema": "^1.4.0",
    "twgl.js": "^4.21.2"
  },
  "devDependencies": {
    "@parcel/transformer-image": "2.0.0-rc.0",
    "cypress": "^8.6.0",
    "parcel": "^2.0.0-rc.0",
    "gh-pages": "^3.2.0"
  }
}
